          <joblisting:advertiseToDateTimeUTC>Tue, 01 Feb 2022 04:59:00</joblisting:advertiseToDateTimeUTC>
          <joblisting:minimumSalary>28.37</joblisting:minimumSalary>
          <joblisting:maximumSalary>38.87</joblisting:maximumSalary>
